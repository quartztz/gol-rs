for line in self.get_cells_as_mut() {
	for cell in line {
		let alive = cell.get_alive();
		let alive_neighbours = self.get_alive_neighbours(cell.get_pos());
		if alive {
			if alive_neighbours < 2 || alive_neighbours > 3 {
				cell.update();
			}
		} else {
			if alive_neighbours == 3 {
				cell.update();
			}
		}
	}
}